{
  "hosting": {
    "public": "dist/angular-seed/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "es/**",
        "destination": "es/index.html"
      } , {
        "source": "en/**",
        "destination": "en/index.html"
      }
    ],
    "redirects" : [
      {
        "source": "/",
        "destination": "/en",
        "type": 301
      }
    ]
  }
}
